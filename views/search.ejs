<div class="author">
    <h1 class="author_heading">Search Results:</h1>
</div>

<section>
    <ul class="article_ul">
        <% data.forEach(post => { %>
         
        <li>
            <a href="/post/<%= post._id %>">
                <span><%= post.title %></span>
                <span class="article_list_date"><%= post.createdAt.toDateString() %></span>
            </a>
        </li>
        <% }) %>
    </ul>
</section>